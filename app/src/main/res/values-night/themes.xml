<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.EventA" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/not_purple_200</item>
        <item name="colorPrimaryVariant">@color/not_purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/not_blue_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="CustomCardview" parent="CardView">
        <item name="cardBackgroundColor">#484848</item>
    </style>

    <style name="ThemeOverlay.EventA.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/black</item>
        <item name="fullscreenTextColor">@color/gray_100</item>
    </style>
</resources>